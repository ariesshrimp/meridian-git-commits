{
  "name": "cz-meridian",
  "version": "1.0.1",
  "description": "A standard commit validator for Meridian projects.",
  "main": "index.js",
  "license": "MIT",
  "bin": "index.js",
  "dependencies": {
    "commitizen": "2.9.6",
    "conventional-github-releaser": "1.1.3",
    "cz-conventional-changelog": "*",
    "github-api": "3.0.0",
    "github-remove-all-releases": "1.0.1",
    "husky": "0.13.2",
    "inquirer": "3.0.6",
    "ramda": "0.23.0",
    "shelljs": "0.7.7",
    "standard-version": "4.0.0",
    "travis-encrypt": "2.1.1"
  },
  "scripts": {
    "build": "rm -rf dist && babel src --out-dir dist --presets=es2015,stage-0 --plugins=transform-runtime",
    "commit": "git cz",
    "start": "babel src --watch --out-dir dist --presets=es2015,stage-0 --plugins=transform-runtime",
    "test": "echo 'test'",
    "release": "node dist/index.js",
    "release:version": "node dist/increment-version.js",
    "release:notes": "node dist/post-version.js",
    "standard": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joefraley/meridian-git-commits.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-stage-0": "6.22.0"
  }
}
