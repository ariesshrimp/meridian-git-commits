{
  "name": "cz-meridian",
  "version": "0.1.0",
  "description": "A standard commit validator for Meridian projects.",
  "main": "index.js",
  "license": "MIT",
  "bin": "index.js",
  "dependencies": {
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "*",
    "conventional-github-releaser": "1.1.3",
    "standard-version": "4.0.0",
    "babel-eslint": "7.1.1",
    "husky": "0.13.2"
  },
  "scripts": {
    "commit": "git cz",
    "test": "echo 'test'",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joefraley/meridian-git-commits.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
